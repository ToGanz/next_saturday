<% if logged_in? %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <%= render 'shared/user_info' %>
      </section>
      <section class="event_form">
        <%= render 'shared/event_form' %>
      </section>
    </aside>
    <div class="col-md-8">
      <h3>Upcoming Events</h3>
      <% if @events.any? %>
        <ol class="events">
          <%= render @events %>
        </ol>
        <%= will_paginate @events %>
      <% end %>
    </div>
  </div>
<% else %>
  <%= render 'shared/home_log_in'%>
<% end %>